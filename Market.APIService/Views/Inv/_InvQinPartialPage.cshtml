@model Market.APIService.Controllers.InvModels
<div class="content-block-title">客户基本信息</div>
<div class="card">
    <div class="card-content">

        <div class="card-content-inner">
            <div class="list-block">
                <ul>
                    <!-- Text inputs -->
                    <!-- Date -->
                    <li>
                        <div class="item-content">
                            <div class="item-media"><i class="icon icon-form-calendar"></i></div>
                            <div class="item-inner">
                                <div class="item-title label">客户名称</div>
                                <div class="item-input">
                                    @Model.CustomerName
                                </div>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="item-content">
                            <div class="item-media"><i class="icon icon-form-calendar"></i></div>
                            <div class="item-inner">
                                <div class="item-title label">客户代号</div>
                                <div class="item-input">
                                    @Model.CustomerCode
                                </div>
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
<div class="content-block-title">库存明细</div>
<div class="card">
    <div class="card-content">
        <div class="card-content-inner">
            <div class="row  no-gutter">
                <div class="col-33">型号</div>
                <div class="col-33">期初库存</div>
                <div class="col-33">库存</div>
                
            </div>
            @{
                foreach (var item in Model.data)
                {
                    <div class="row  no-gutter">
                        <div class="col-33">@item.modelCode</div>
                        @{
                            if (@item.qty1.Equals("无"))
                            {
                                <div class="col-33">&nbsp;</div>
                            }
                            else
                            {
                                <div class="col-33">@item.qty1</div>
                            }
                        }
                        @{
                            if (@item.qty2.Equals("0"))
                            {
                                <div class="col-33">&nbsp;</div>
                            }
                            else
                            {
                                <div class="col-33">@item.qty2</div>
                            }
                        }
                       
                       
                       
                    </div>
                }
            }



                       
                    

        </div>
    </div>
</div>