@model List<Market.APIService.Controllers.MemberListModel>
<div class="content-block-title">会员列表</div>
<div class="list-block media-list">
    @{
        foreach (var item in Model)
        {
            <ul>
                <li>
                    <a href="#" class="item-link item-content" onclick="CheckItem('@item.mf_memberNo')">
                        <div class="item-media"><img src="@item.pic_Url" style='width: 4rem;'></div>
                        <div class="item-inner">
                            <div class="item-title-row">
                                <div class="item-title">@item.time</div>
                            </div>
                            <div class="item-subtitle">@item.name</div>

                            
                            <div class="item-text">到访门店:@item.shop</div>
                        </div>
                    </a>
                </li>
            </ul>
        }
    }
</div>

   


           
        