                       + '     <div style="width:100%;height:15px;"></div>'
                       + '     <div style="width:100%;height:15px;font-size:15px;color:#24acca">' + item.userName + '</div>'
                       + '     <div style="width:100%;height:10px;"></div>'
                       + '     <div style="width:100%;height:10px;font-size:10px;">假别  ' + item.leaveType + '</div>'
                       + '     <div style="width:100%;height:5px;"></div>'
