               null,
               function (data) {
                   

                   var html1 = '';
                   $$("#type_view_1").html(html1);
                   var html2 = '';
                   for (var i = 0; i < data.type1List.length;i++)
                   {
                       var item = data.type1List[i];

                       var type = item.leaveType;
                       var StarDay = item.startDay + '~';
                       var EndDay = item.endDay;
                       html2 = html2 + '<div onclick="window.location = \'MemberLeveOrderIndex?userId=' + item.userId + '&id=' + item.id + '&aUserId=' + '@userId' + '\'"  style="width:100%;height:89px;display:flex;border-bottom:solid;border-color:#dddddd;border-width:1px;">'
