                        + '<div style="width:69px;height:89px;float:left;color:#000000;font-size:14px;line-height:50px;text-align:center;">'
                        + '    <div style="width:100%;height:10px;"></div>'
                         + '   <div style="width:100%;height:69px;">'
                          + '      <img src="https://iretailerapp.flnet.com/Content/Img/tab_user_active.png" style="width:69px; height:69px;" />'
