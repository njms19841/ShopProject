                        + '    <div style="width:100%;height:10px;"></div>'
                         + '   <div style="width:100%;height:69px;">'
                          + '      <img src="https://iretailerapp.flnet.com/Content/Img/tab_user_active.png" style="width:69px; height:69px;" />'
                          + '  </div>'
                       + ' </div>'
                       + ' <div style="width:10px;height:89px;float:left;color:#000000;font-size:14px;line-height:50px;text-align:center;"></div>'
                       + ' <div style="flex:1;height:89px;float:left;color:#000000;font-size:14px;text-align:left;">'
                       + '     <div style="width:100%;height:15px;"></div>'
                       + '     <div style="width:100%;height:15px;font-size:15px;color:#24acca">' + item.userName+ '</div>'
                       + '     <div style="width:100%;height:10px;"></div>'
                       + '     <div style="width:100%;height:10px;font-size:10px;">假别  ' + item.leaveType + '</div>'
                       + '     <div style="width:100%;height:5px;"></div>'
                        + '    <div style="width:100%;height:10px;font-size:10px;">时间  ' + item.startDay + '~' + item.endDay + '</div>'
                       + ' </div>'
                      + '  <div style="width:25px;height:89px;float:left;color:#000000;font-size:14px;">'
                       + '     <div style="width:100%;height:35px;display:flex;">'
                      + '      </div>'
                       + '     <div style="width:15px; height:15px;">'
                       + '         <img src="https://iretailerapp.flnet.com/Content/Img/img_next.png" style="width:15px; height:15px; " />'
                       + '     </div>'
                      + '  </div>'
                   + ' </div>';
                   }

                   $$("#type_view_1").html(html2);

                   var html3 = '';
                   for (var i = 0; i < data.type2List.length; i++) {
                       var item = data.type2List[i];
                       var type = item.leaveType;
                       var StarDay = item.startDay + '~';
                       var EndDay = item.endDay;
                       html3 = html3 + '<div onclick="window.location = \'MemberLeveOrderIndex?userId=' + item.userId + '&id=' + item.id + '&aUserId=' + '@userId' + '\'"  style="width:100%;height:89px;display:flex;border-bottom:solid;border-color:#dddddd;border-width:1px;">'
